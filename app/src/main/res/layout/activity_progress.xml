<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.example.dev.CustomDonutProgress
        android:id="@+id/recorder_donut_progress"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="@dimen/bdp_50"
        android:rotation="270"
        app:donut_finished_color="#ffa800"
        app:donut_finished_stroke_width="4dp"
        app:donut_max="15"
        app:donut_progress="5"
        app:donut_text_color="#00000000"
        app:donut_unfinished_color="#ffffff"
        app:donut_unfinished_stroke_width="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>